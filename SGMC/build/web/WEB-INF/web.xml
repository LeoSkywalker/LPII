<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PesquisaClienteController</servlet-name>
        <servlet-class>controller.PesquisaClienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaColaboradorController</servlet-name>
        <servlet-class>controller.PesquisaColaboradorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaFornecedorController</servlet-name>
        <servlet-class>controller.PesquisaFornecedorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaProdutoController</servlet-name>
        <servlet-class>controller.PesquisaProdutoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaVendaController</servlet-name>
        <servlet-class>controller.PesquisaVendaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaFormaPagamentoController</servlet-name>
        <servlet-class>controller.PesquisaPagamentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaOrdemServicoController</servlet-name>
        <servlet-class>controller.PesquisaOrdemServicoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisaClienteController</servlet-name>
        <url-pattern>/PesquisaClienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaColaboradorController</servlet-name>
        <url-pattern>/PesquisaColaboradorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFornecedorController</servlet-name>
        <url-pattern>/PesquisaFornecedorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaProdutoController</servlet-name>
        <url-pattern>/PesquisaProdutoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaVendaController</servlet-name>
        <url-pattern>/PesquisaVendaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFormaPagamentoController</servlet-name>
        <url-pattern>/PesquisaPagamentoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaOrdemServicoController</servlet-name>
        <url-pattern>/PesquisaOrdemServicoController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
